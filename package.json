{
  "name": "ahj_loading_styling",
  "version": "1.0.0",
  "description": "WebWorkers, ServiceWorkers",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint .",
    "test": "jest --testPathPattern=./src/",
    "coverage": "jest --testPathPattern=./src/ --coverage",
    "e2e": "jest --testPathPattern=./e2e/ --runInBand"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.html?$": "html-loader-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "src/js/__tests__/e2e/e2e.server.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polinkavishinka/ahj_loadin_styling.git"
  },
  "author": "polinkavishinka",
  "license": "ISC",
  "homepage": "https://polinkavishinka.github.io/ahj_loading_styling#readme",
  "bugs": {
    "url": "https://github.com/polinkavishinka/ahj_loading_styling/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "babel-jest": "^27.2.4",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.7",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-restricted-globals": "^0.2.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-loader-jest": "^0.2.1",
    "html-webpack-plugin": "^4.5.2",
    "jest": "^27.2.4",
    "jsdoc": "^3.6.7",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^1.6.2",
    "optimize-css-assets-webpack-plugin": "^5.0.8",
    "postcss": "^8.3.11",
    "puppeteer": "^10.4.0",
    "terser-webpack-plugin": "^5.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.57.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0",
    "workbox-webpack-plugin": "^6.4.1",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@yarnpkg/pnpify": "^3.0.1-rc.9",
    "buffer": "^6.0.3",
    "core-js": "^3.18.3",
    "csv-parser": "^3.0.0",
    "push-dir": "^0.4.1",
    "rxjs": "^7.4.0",
    "stream-browserify": "^3.0.0"
  }
}
